          <li <% if (locals.page === "suggested") { %> class="active"<% } %>>
            <a href="/cite/suggested"><i class="icon-search"></i> Suggested Sources</a>
          </li>
          <li class="nav-header">Finalize</li>
          <li <% if (locals.page === "review") { %> class="active"<% } %>>
            <a href="/cite/review"><i class="icon-list-alt"></i> Review Sources</a>
          </li>
        </ul>
      </div>
    </div>
    <div id="mainpage" class="clearfix">
      <div class="span9">
        <%- body %>
      </div>
    </div>
  </div>
